            <targetProductNameSuffix>_wind</targetProductNameSuffix>
            <placeAfter>OilSpillDetectionOp</placeAfter>
        </action>


        <action>
            <id>ClusteringGroup</id>
            <class>org.esa.snap.visat.actions.ActionGroup</class>
